# Streamlit Secrets Configuration Template
# Copy this file to secrets.toml and fill in your actual values
# DO NOT commit secrets.toml to version control

# Database Configuration for Supabase
[database]
DB_HOST = "db.your-project.supabase.co"
DB_PORT = "6543"
DB_NAME = "postgres"
DB_USER = "postgres"
DB_PASSWORD = "your-supabase-password"
DB_SCHEMA = "test"  # Default schema to use (optional, defaults to "public")

# Alternative: Use connection string format
# connection_string = "postgresql://postgres:password@db.project.supabase.co:6543/postgres?sslmode=require"
